	"version": "2.1.0",
	"homepage": "https://discord.gg/Kf7BzSw",
	"license": "MIT",
	"private": true,
	"scripts": {
		"build": "vite build && node scripts/build.mjs",
		"dev": "concurrently \"node scripts/build.mjs --dev\" \"vite dev\"",
